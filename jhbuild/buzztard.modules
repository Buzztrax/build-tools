<?xml version="1.0" standalone="no"?> <!--*- mode: nxml -*-->

<!DOCTYPE moduleset SYSTEM "moduleset.dtd">
<?xml-stylesheet type="text/xsl" href="moduleset.xsl"?>
<moduleset>
  <repository type="svn" name="buzztard" href="https://buzztard.svn.sourceforge.net/svnroot/"/>

  <autotools id="bml">
    <branch repo="buzztard" module="buzztard/trunk/bml" checkoutdir="bml"/>
  </autotools>

  <autotools id="buzzmachines">
    <dependencies>
      <dep package="bml"/>
    </dependencies>
    <branch repo="buzztard" module="buzztard/trunk/buzzmachines" checkoutdir="buzzmachines"/>
  </autotools>

  <autotools id="gst-buzztard">
    <dependencies>
      <dep package="buzzmachines"/>
    </dependencies>
    <branch repo="buzztard" module="buzztard/trunk/gst-buzztard" checkoutdir="gst-buzztard"/>
  </autotools>

  <autotools id="buzztard">
    <dependencies>
      <dep package="gst-buzztard"/>
    </dependencies>
    <branch repo="buzztard" module="buzztard/trunk/buzztard" checkoutdir="buzztard"/>
  </autotools>

  <autotools id="bsl">
    <dependencies>
      <dep package="buzztard"/>
    </dependencies>
    <branch repo="buzztard" module="buzztard/trunk/bsl" checkoutdir="bsl"/>
  </autotools>


  <metamodule id="buzztard-all">
    <dependencies>
      <dep package="bsl"/>
    </dependencies>
  </metamodule>

</moduleset>
